    "eslint": "^9.33.0",
    "eslint-plugin-oxlint": "~1.11.0",
    "eslint-plugin-vue": "~10.4.0",
    "jiti": "^2.5.1",
    "npm-run-all2": "^8.0.4",
    "oxlint": "~1.11.0",
    "prettier": "3.6.2",
    "typescript": "~5.9.0",
    "vite": "npm:rolldown-vite@latest",
    "vite-plugin-vue-devtools": "^8.0.2",
    "vue-tsc": "^3.1.0"
