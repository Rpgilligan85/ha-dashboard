    "lodash": "^4.17.21",
    "material-symbols": "^0.38.0",
    "pinia": "^3.0.3",
    "primeflex": "^4.0.0",
    "primeicons": "^7.0.0",
    "primevue": "^4.4.1",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.15",
    "tailwindcss-primeui": "^0.6.1",
    "vue": "^3.5.22",
    "vue-router": "^4.5.1"
